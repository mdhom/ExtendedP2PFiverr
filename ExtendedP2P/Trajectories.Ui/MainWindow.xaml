<Window x:Class="Trajectories.Ui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Trajectories.Ui"
        mc:Ignorable="d"
        Title="Trajectories.Ui" Height="1000" Width="1000">
    <Grid>
        <TabControl Margin="10">
            <TabItem Header="To Velocity" Padding="5">
                <local:TrajectoryToVelocityView Margin="10" />
            </TabItem>
            <TabItem Header="To Distance" Padding="5">
                <local:TrajectoryToDistanceView Margin="10" />
            </TabItem>
        </TabControl>
    </Grid>
</Window>
